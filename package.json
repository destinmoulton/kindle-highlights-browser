{
  "name": "kindle-highlights-browser",
  "version": "0.0.4",
  "description": "A Kindle highlights viewer. Built using Node, Electron, and React.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile-react": "webpack --config webpack.config.js -d --watch",
    "app": "electron .",
    "compile-windows": "electron-packager . --overwrite --out=dist/win32 --platform=win32 --arch=x64 --electron-version=1.4.15 --icon=assets/icons/BlackvariantKindle.ico --ignore=assets --ignore=typings --ignore=.vscode --ignore=dist --ignore=node_modules",
    "compile-osx":"electron-packager . --overwrite --out=dist/osx --platform=darwin --arch=x64 --electron-version=1.4.15 --icon=assets/icons/BlackvariantKindle.icns --ignore=assets --ignore=typings --ignore .vscode --ignore=dist --ignore=node_modules"
  },
  "keywords": [
    "electron",
    "js",
    "node",
    "react"
  ],
  "author": "Destin Moulton",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "electron": "^1.4.15",
    "electron-packager": "^8.5.1",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "moment": "^2.17.1"
  }
}
